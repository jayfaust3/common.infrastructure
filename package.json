{
  "name": "aws-cdk",
  "version": "0.1.0",
  "description": "Responsible for the provisioning of core AWS resources",
  "scripts": {
    "synth": "cdklocal synth",
    "bootstrap": "cdklocal bootstrap",
    "deploy": "cdklocal deploy"
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": [
        "tsconfig.json"
      ]
    }
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "2.103.1-alpha.0",
    "@aws-cdk/aws-redshift-alpha": "2.103.1-alpha.0",
    "aws-cdk-lib": "2.103.1",
    "constructs": "10.3.0",
    "dotenv": "16.3.1"
  },
  "devDependencies": {
    "@types/node": "20.3.2",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "eslint": "7.32.0",
    "ts-node": "10.8.1",
    "typescript": "4.7.4"
  }
}
